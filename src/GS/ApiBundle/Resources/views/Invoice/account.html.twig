{% set account = invoice.payment.account %}
{% set address = account.address %}

<div class="col-md-12 col-xs-12" style="border:1px solid; border-radius:5px">
    <h4>{{ account.firstName }} {{ account.lastName }}</h4>
    <p>
        {{ include("GSApiBundle:Invoice:address.html.twig") }}
        Tel: {{ phone_number_format(account.phoneNumber) }}<br>
        Email: {{ account.email }}<br>
    </p>
</div>